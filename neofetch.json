{
  "name": "neofetch",
  "version": "7.1.0",
  "install_script": "wget https://github.com/dylanaraps/neofetch/archive/refs/tags/7.1.0.tar.gz && tar -xvf 7.1.0.tar.gz && cd 7.1.0 && ./install.sh",
  "update_script": "git clone https://github.com/dylanaraps/neofetch",
  "check_update_script": "git fetch && git log -1 --pretty=format:%H",
  "uninstall_script": "rm -rf /usr/local/bin/neofetch && rm -rf /usr/local/share/neofetch"
}
